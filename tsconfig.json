{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/FormTextInput.jsx", "src/components/Cart.jsx", "src/pages/api/webhooks.js", "src/pages/api/checkout_session.js", "src/components/Navbar.tsx", "src/components/Cart/CartList.tsx"],
  "exclude": ["node_modules"]
}
